    "gatsby-plugin-google-tagmanager": "^2.8.0",
    "gatsby-plugin-hotjar": "^1.1.1",
    "gatsby-plugin-manifest": "^2.9.1",
    "gatsby-plugin-offline": "^3.7.1",
    "gatsby-plugin-page-creator": "^3.12.0",
    "gatsby-plugin-react-helmet": "^3.7.0",
    "gatsby-plugin-segment-js": "^3.6.0",
    "gatsby-plugin-sharp": "^2.14.4",
    "gatsby-plugin-sitemap": "^2.9.0",
    "gatsby-plugin-styled-components": "^3.7.0",
    "gatsby-source-contentful": "^4.3.1",
    "gatsby-source-filesystem": "^2.8.1",
    "gatsby-transformer-remark": "^2.13.1",
    "gatsby-transformer-sharp": "^2.9.0",
    "html-react-parser": "^1.1.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
