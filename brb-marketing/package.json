    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-mailchimp-subscribe": "^2.1.0",
    "react-markdown": "^5.0.3",
    "react-moment-countdown": "^0.4.2",
    "react-moment-proptypes": "^1.7.0",
    "react-typeform-embed": "^0.2.1",
    "reading-time": "^1.2.1",
    "smooth-scroll": "^16.1.3",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:start": "docker run -ti -v $(pwd):/app --user \"$(id -u):$(id -g)\" --workdir=/app -p 8000:8000 node:16.13-bullseye sh -c 'yarn run start'",
    "docker:build": "docker run -t  -v $(pwd):/app --user \"$(id -u):$(id -g)\" --workdir=/app node:16.13-bullseye sh -c 'yarn run build'",
    "docker:shell": "docker run -ti -v $(pwd):/app --user \"$(id -u):$(id -g)\" --workdir=/app node:16.13-bullseye sh",
    "docker:reset": "docker run -ti -v $(pwd):/app --user \"$(id -u):$(id -g)\" --workdir=/app node:16.13-bullseye sh -c 'rm -rf node_modules && yarn install --pure-lockfile && rm -f .eslintcache'"
  },
  "devDependencies": {
    "prettier": "^2.2.1"
  }
}
